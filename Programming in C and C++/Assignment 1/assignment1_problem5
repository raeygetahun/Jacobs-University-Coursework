#include <stdio.h>

int main(){

    int x = 2138;
    char z = 'G';
    float y = -52.358925;
    double u = 61.295339687;
    
     printf("x=%9i\n", x);
     printf("y=%11.5f\n",y);
     printf("z=%c\n", z);
     printf("u=%.7f\n", u); /*prints the value of u
           with a floating point precision of 7.*/
    return 0;
    
}
